voz (grand_piano)
{
    repetir (8)
    {
        compas
        {
            silencio(redonda.);
        }
    }
    repetir(2)
    {
    compas
    {
        nota(re, octava_mas_alta, redonda);
        silencio(negra.);
        nota(do, octava_mas_alta, corchea);
    }
   
}

